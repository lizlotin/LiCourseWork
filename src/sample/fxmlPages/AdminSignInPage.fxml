<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: F5F5DC;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminSignInPage">
   <children>
      <Label layoutX="184.0" layoutY="30.0" text="SEVEN WONDERS OF UKRAINE">
         <font>
            <Font name="Bodoni MT Bold" size="58.0" />
         </font>
      </Label>
      <Label fx:id="notTour" alignment="CENTER" layoutX="388.0" layoutY="619.0" prefHeight="42.0" prefWidth="462.0" text="Your request sended! Thanks!" textAlignment="CENTER" visible="false">
         <font>
            <Font name="Courier New Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="signIn" layoutX="564.0" layoutY="370.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="123.0" style="-fx-background-radius: 30px; -fx-background-color: f5f5dc;" text="Sign In!">
         <font>
            <Font name="Bodoni MT Bold" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="461.0" layoutY="112.0" text="Tours in Ukraine">
         <font>
            <Font name="Bodoni MT" size="43.0" />
         </font>
      </Label>
      <Label layoutX="494.0" layoutY="175.0" text="admin sign in">
         <font>
            <Font name="Bodoni MT Bold" size="43.0" />
         </font>
      </Label>
      <TextField fx:id="loginField" alignment="CENTER" layoutX="532.0" layoutY="263.0" promptText="login">
         <font>
            <Font size="15.0" />
